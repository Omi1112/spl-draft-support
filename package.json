{
  "name": "my-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix"
    ]
  },
  "scripts": {
    "dev": "WATCHPACK_POLLING=true next dev",
    "build": "next build",
    "vercel-build": "next build && prisma migrate deploy",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint-staged": "lint-staged",
    "postinstall": "prisma generate",
    "prepare": "husky && playwright install && playwright install-deps",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "test:typecheck": "npm run typecheck && npm test"
  },
  "dependencies": {
    "@graphql-tools/schema": "^10.0.23",
    "@prisma/client": "^6.5.0",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^5.13.2",
    "jest-mock-extended": "^4.0.0-beta1",
    "next": "15.2.4",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "urql": "^4.2.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@swc/core": "^1.11.18",
    "@swc/jest": "^0.2.37",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.24.0",
    "eslint-config-next": "15.2.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "tailwindcss": "^4",
    "typescript": "^5.8.3"
  }
}
